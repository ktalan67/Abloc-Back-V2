{% extends 'base.html.twig' %}

{% block title %}MasteryLevel index{% endblock %}

{% block body %}
    <h1>MasteryLevel index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Created_at</th>
                <th>Updated_at</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for mastery_level in mastery_levels %}
            <tr>
                <td>{{ mastery_level.id }}</td>
                <td>{{ mastery_level.title }}</td>
                <td>{{ mastery_level.createdAt ? mastery_level.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ mastery_level.updatedAt ? mastery_level.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('mastery_level_show', {'id': mastery_level.id}) }}">show</a>
                    <a href="{{ path('mastery_level_edit', {'id': mastery_level.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('mastery_level_new') }}">Create new</a>
{% endblock %}
